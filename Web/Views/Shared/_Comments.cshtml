@model CommentsView

<ul class="list-unstyled" id="comments-list">
    @foreach (var i in Model.Comments)
    {
        <li class="media">
            <div class="media-body">
                <h5 class="mt-0 mb-1">@i.AppUser.UserName</h5>
                @i.Text
            </div>
        </li>
    }
</ul>